<article class="col-sm-12">
    <div class="jarviswidget">
        <header>
            <span class="widget-icon">
                <i class="fa fa-history"></i>
            </span>
            <h2>Gráfico de Acessos por Mês</h2>
        </header>
        <div>
            <div class="widget-body">
                <div class="row">
                    <div class="col-md-12">
                       <div id="saleschart" class="chart" style="padding: 0px; position: relative;">
                            <canvas class="flot-base" width="521" height="220" style="direction: ltr; position: absolute; left: 0px; top: 0px; width: 521px; height: 220px;"></canvas>
                            <div class="flot-text" style="position: absolute; top: 0px; left: 0px; bottom: 0px; right: 0px; font-size: smaller; color: rgb(84, 84, 84);">
                                <div class="flot-x-axis flot-x1-axis xAxis x1Axis" style="position: absolute; top: 0px; left: 0px; bottom: 0px; right: 0px; display: block;">
                                    <div class="flot-tick-label tickLabel" style="position: absolute; max-width: 74px; top: 205px; left: 18px; text-align: center;">Dec 01</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; max-width: 74px; top: 205px; left: 100px; text-align: center;">Dec 16</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; max-width: 74px; top: 205px; left: 188px; text-align: center;">Jan 01</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; max-width: 74px; top: 205px; left: 270px; text-align: center;">Jan 16</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; max-width: 74px; top: 205px; left: 355px; text-align: center;">Feb 01</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; max-width: 74px; top: 205px; left: 432px; text-align: center;">Feb 15</div>
                                </div>
                                <div class="flot-y-axis flot-y1-axis yAxis y1Axis" style="position: absolute; top: 0px; left: 0px; bottom: 0px; right: 0px; display: block;">
                                    <div class="flot-tick-label tickLabel" style="position: absolute; top: 183px; left: 19px; text-align: right;">0</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; top: 147px; left: 0px; text-align: right;">1000</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; top: 110px; left: 0px; text-align: right;">2000</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; top: 75px; left: 0px; text-align: right;">3000</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; top: 39px; left: 0px; text-align: right;">4000</div>
                                    <div class="flot-tick-label tickLabel" style="position: absolute; top: 3px; left: 0px; text-align: right;">5000</div>
                                </div>
                            </div>
                            <canvas class="flot-overlay" width="521" height="220" style="direction: ltr; position: absolute; left: 0px; top: 0px; width: 521px; height: 220px;"></canvas>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</article>
