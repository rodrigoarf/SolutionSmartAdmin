<!DOCTYPE html>
<html lang="en-us">	
	<head>		
		<title>SmartClinic</title>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black">	
			
		<link rel="stylesheet" type="text/css" media="screen" href="~/Content/css/bootstrap.min.css">
		<link rel="stylesheet" type="text/css" media="screen" href="~/Content/css/font-awesome.min.css">
		<link rel="stylesheet" type="text/css" media="screen" href="~/Content/css/smartadmin-production.min.css">
		<link rel="stylesheet" type="text/css" media="screen" href="~/Content/css/smartadmin-skins.min.css">
		<link rel="stylesheet" type="text/css" media="screen" href="~/Content/css/demo.min.css">

		<link rel="shortcut icon" href="~/Content/img/favicon/favicon.ico" type="image/x-icon">
		<link rel="icon" href="~/Content/img/favicon/favicon.ico" type="image/x-icon"> 
		<link rel="apple-touch-icon" href="~/Content/img/splash/sptouch-icon-iphone.png">
		<link rel="apple-touch-icon" sizes="76x76" href="~/Content/img/splash/touch-icon-ipad.png">
		<link rel="apple-touch-icon" sizes="120x120" href="~/Content/img/splash/touch-icon-iphone-retina.png">
		<link rel="apple-touch-icon" sizes="152x152" href="~/Content/img/splash/touch-icon-ipad-retina.png"> 
		<link rel="apple-touch-startup-image" href="~/Content/img/splash/ipad-landscape.png" media="screen and (min-device-width: 481px) and (max-device-width: 1024px) and (orientation:landscape)">
		<link rel="apple-touch-startup-image" href="~/Content/img/splash/ipad-portrait.png" media="screen and (min-device-width: 481px) and (max-device-width: 1024px) and (orientation:portrait)">
		<link rel="apple-touch-startup-image" href="~/Content/img/splash/iphone.png" media="screen and (max-device-width: 320px)">
        <style>
            .smart-admin-loading {                  
                overflow: hidden;
                position: fixed;
                top: 0;
                right: 0;
                bottom: 0;
                left: 0;
                z-index: 9999;
                -webkit-overflow-scrolling: touch;
                outline: 0;
                background: #FFF;
                opacity: 0.75;
                text-align:center; 
            }
            .smart-admin-loading:before {
                content: "";
                display: inline-block;
                height: 100%;
                vertical-align: middle;
            }           
            .smart-admin-loading img {
                vertical-align: middle;
            }
        </style>
	</head>


	<!-- Possible Classes 
		* 'smart-skin-{SKIN#}'
		* 'smart-rtl'         - Switch theme mode to RTL (will be avilable from version SmartAdmin 1.5)
		* 'menu-on-top'       - Switch to top navigation (no DOM change required)
		* 'hidden-menu'       - Hides the main menu but still accessable by hovering over left edge
		* 'fixed-header'      - Fixes the header
		* 'fixed-navigation'  - Fixes the main menu
		* 'fixed-ribbon'      - Fixes breadcrumb
		* 'fixed-footer'      - Fixes footer
		* 'container'         - boxed layout mode (non-responsive: will not work with fixed-navigation & fixed-ribbon)
	-->
	<body>
        <div id="smart-admin-loading" class="smart-admin-loading" style="display:none;" >
            <img src="~/Content/img/loader/loader2.gif" />
        </div>     
		<!-- #HEADER -->
		<header id="header">
			 @Html.Partial("_HeaderPartial")	
		</header>

		<!-- END HEADER -->

		<!-- #NAVIGATION -->
		<aside id="left-panel">
		    @Html.Action("MainMenu","Menu")
		</aside>
		<!-- END NAVIGATION -->

		<!-- #MAIN PANEL -->
		<div id="main" role="main">
			<div id="ribbon">
				@Html.Partial("_RibbonPartial")
			</div>

			<!-- SCRIPTS --> 
			<script src="~/Content/js/libs/jquery-2.0.2.min.js"></script>
			<script src="~/Content/js/libs/jquery-ui-1.10.3.min.js"></script> 
			<script src="~/Content/js/bootstrap/bootstrap.min.js"></script>
			<script src="~/Content/js/plugin/bootstrap-slider/bootstrap-slider.min.js"></script>
			<script src="~/Content/js/plugin/jquery-touch/jquery.ui.touch-punch.min.js"></script>
			<script src="~/Content/js/notification/SmartNotification.min.js"></script>
			<script src="~/Content/js/smartwidgets/jarvis.widget.min.js"></script>
			<script src="~/Content/js/plugin/easy-pie-chart/jquery.easy-pie-chart.min.js"></script>
			<script src="~/Content/js/plugin/sparkline/jquery.sparkline.min.js"></script>
			<script src="~/Content/js/plugin/jquery-validate/jquery.validate.min.js"></script>
			<script src="~/Content/js/plugin/masked-input/jquery.maskedinput.min.js"></script>  
            <script src="~/Content/js/plugin/masked-money/jquery.maskmoney.min.js"></script>  
			<script src="~/Content/js/plugin/msie-fix/jquery.mb.browser.min.js"></script>
			<script src="~/Content/js/plugin/fastclick/fastclick.min.js"></script>
			<script src="~/Content/js/plugin/jquery-icheck/jquery.icheck.js"></script> 
            <script src="~/Content/js/plugin/jquery-cookie/jquery.cookie.js"></script>
			<script src="~/Content/js/app.config.js"></script>
			<script src="~/Content/js/demo.min.js"></script>
			<script src="~/Content/js/app.min.js"></script> 
			<!-- END SCRIPTS -->
           
			<div id="content">                           
                @RenderBody()
			</div>
		</div>
		<!-- END #MAIN PANEL -->

		<!-- #PAGE FOOTER -->
		<div class="page-footer">
			  @Html.Partial("_FooterPartial")	
		</div>
		<!-- END FOOTER -->


		<!-- #SHORTCUT AREA -->
		<div id="shortcut">
			  @Html.Partial("_WidgetPartial")  
		</div>

		<!--[if IE 8]>
			<h1>Seu navegador está desatualizado, por favor atualize seu navegador, indo para www.microsoft.com/download</h1>
		<![endif]--> 

	</body>
</html>
