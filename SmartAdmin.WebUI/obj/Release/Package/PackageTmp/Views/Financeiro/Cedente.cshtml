@model SmartAdmin.Dto.CedenteDto
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-4">
        <h1 class="page-title txt-color-blueDark">
            <i class="fa-fw fa fa-home"></i>Dados do Cedente
        </h1>
    </div>
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-8 text-right"></div>

    <div class="col-md-12">
        <div class="row">
            <article class="col-sm-12">
                <div class="jarviswidget jarviswidget-color-blueDark" id="widget-create">
                   @Html.Partial("CedenteEditPartial", Model)
                </div>
            </article>
        </div>
    </div>
</div>  

<script src="/Script/Financeiro/Cedente.js" type="text/javascript"></script>
<script type="text/javascript">  
    @((!String.IsNullOrEmpty(ViewBag.Mensagem)) ? Html.Raw("PrintAlert(\"" + ViewBag.Mensagem + "\");") : null) 
</script>


